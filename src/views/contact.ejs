<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head', { headTitle: "Contact | EJS Exercise"}) %>
  <body>
    <div class="container">
      <%- include('partials/header', { title}) %>
      <main>
        <p><%= pageDetail %></p>
        <ul class="contact-list">
          <% contactsWay.forEach(way => { %>
          <li>
            <div>Mail: <%= way.mail %></div>
            <div>Phone: <%= way.tell %></div>
            <div>Location: <%= way.location %></div>
          </li>
          <% }) %>
        </ul>
      </main>
      <%- include('partials/footer') %>
    </div>
  </body>
</html>
